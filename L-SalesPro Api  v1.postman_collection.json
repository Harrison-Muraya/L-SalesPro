					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Save access token to environment\r",
									"var jsonData = pm.response.json();\r",
									"if (jsonData.success && jsonData.data.token) {\r",
									"    pm.environment.set(\"access_token\", jsonData.data.token);\r",
									"}\r",
									"\r",
									"// Test assertions\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response has token\", function () {\r",
									"    pm.expect(jsonData.data).to.have.property('token');\r",
